{
  "name": "collab-canvas",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "deploy": "npm run build && firebase deploy",
    "deploy:hosting": "npm run build && firebase deploy --only hosting",
    "deploy:vercel": "npm run build && npx vercel --prod --yes && echo '⚠️ REMINDER: Update alias! See DEPLOYMENT_GUIDE.md'"
  },
  "dependencies": {
    "@vercel/node": "^5.4.0",
    "firebase": "^12.4.0",
    "konva": "^10.0.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-konva": "^19.0.10",
    "react-router-dom": "^7.9.4"
  },
  "devDependencies": {
    "@ai-sdk/openai": "^2.0.52",
    "@eslint/js": "^9.36.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react-swc": "^4.1.0",
    "@vitest/ui": "^3.2.4",
    "ai": "^5.0.76",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "express": "^5.1.0",
    "globals": "^16.4.0",
    "jsdom": "^27.0.0",
    "terser": "^5.44.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7",
    "vitest": "^3.2.4",
    "zod": "^4.1.12"
  }
}
